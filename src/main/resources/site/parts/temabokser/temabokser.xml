<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
    <display-name>Temabokser</display-name>
    <description>Bokser med temaer for selvhjelo</description>
    <form>
        <input name="title" type="TextLine">
            <label>Title</label>
            <occurrences minimum="1" maximum="1"/>
            <default>My title</default>
        </input>
        <input name="text" type="TextLine">
            <label>Beskrivelse</label>
            <occurrences minimum="1" maximum="1"/>
            <default>Lorem ipsum dolor sit amnet</default>
        </input>
    </form>
</part>