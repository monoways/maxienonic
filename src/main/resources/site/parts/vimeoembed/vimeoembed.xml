<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
    <display-name>Vimeo Video</display-name>
    <description>Embed av vimeo video</description>
    <form>
        <input name="title" type="TextLine">
            <label>Title</label>
            <occurrences minimum="1" maximum="1"/>
            <default>Vimeo 1</default>
        </input>
        <input name="text" type="TextLine">
            <label>Beskrivelse</label>
            <occurrences minimum="1" maximum="1"/>
            <default>Lorem ipsum dolor sit amnet</default>
        </input>
        <input name="videoId" type="TextLine">
            <label>Video ID</label>
            <occurrences minimum="1" maximum="1"/>
            <default>788973084?h=86d1fde358</default>
        </input>
    </form>
</part>